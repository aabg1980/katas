<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="300.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.javeriana.sistemas.fundamentos.registro.RegistroController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Pane prefHeight="357.0" prefWidth="260.0">
      <children>
         <VBox layoutX="7.0" layoutY="37.0" prefHeight="521.0" prefWidth="246.0">
            <children>
               <Label alignment="TOP_LEFT" contentDisplay="TEXT_ONLY" prefHeight="54.0" prefWidth="246.0" text="Bienvenido al registro de usuarios &#10;JaveBank" textAlignment="JUSTIFY" textFill="#0837e3">
                  <effect>
                     <SepiaTone />
                  </effect>
               </Label>
               <Label text="Nombres:" />
               <TextField fx:id="nombres" alignment="TOP_LEFT" prefHeight="27.0" prefWidth="245.0" promptText="Nombres" />
               <Label text="Apellidos:" />
               <TextField fx:id="apellidos" prefHeight="27.0" prefWidth="245.0" promptText="Apellidos" />
               <Label text="Tipo de Documento:" />
               <ComboBox fx:id="tipoDocumento" prefHeight="27.0" prefWidth="245.0" visibleRowCount="5" />
               <Label text="Numero de Documento:" />
               <TextField fx:id="numeroDocumento" prefWidth="245.0" promptText="Numero de Documento" />
               <Label text="Fecha de Nacimiento:" />
               <DatePicker fx:id="fechaNacimiento" prefHeight="27.0" prefWidth="245.0" />
               <Label text="Genero:" />
               <ComboBox fx:id="genero" prefWidth="245.0" />
               <Label text="Correo:" />
               <TextField fx:id="email" prefWidth="245.0" promptText="email" />
               <Label text="Telefono:" />
               <TextField fx:id="telefono" prefWidth="245.0" promptText="Telefono" />
                     <Label fx:id="welcomeText" prefHeight="27.0" prefWidth="260.0" />
                     <Button onAction="#onGuardarButtonClick" text="Registrar Usuario" />
            </children>
         </VBox>
      </children>
   </Pane>
</VBox>
